{
  "name": "can-view-autorender",
  "version": "3.0.3",
  "description": "Automatically render templates found in the document",
  "homepage": "https://canjs.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/canjs/can-view-autorender.git"
  },
  "author": {
    "name": "Bitovi",
    "email": "contact@bitovi.com",
    "url": "http://bitovi.com"
  },
  "scripts": {
    "preversion": "npm test && npm run build",
    "version": "git commit -am \"Update dist for release\" && git checkout -b release && git add -f dist/",
    "postversion": "git push --tags && git checkout master && git branch -D release && git push",
    "testee": "testee test/test.html --browsers firefox",
    "test": "npm run jshint && npm run testee",
    "jshint": "jshint ./*.js --config",
    "release:pre": "npm version prerelease && npm publish",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "build": "node build.js",
    "document": "bit-docs",
    "develop": "done-serve --static --develop --port 8080"
  },
  "main": "can-view-autorender",
  "keywords": [
    "canjs",
    "canjs-plugin",
    "donejs"
  ],
  "system": {
    "configDependencies": [
      "live-reload"
    ],
    "npmAlgorithm": "flat"
  },
  "dependencies": {
    "can-event": "^3.0.1",
    "can-util": "^3.2.2",
    "can-view-model": "^3.0.0"
  },
  "devDependencies": {
    "can-component": "^3.0.0-pre.3",
    "can-define": "^1.0.1",
    "can-stache": "^3.0.0-pre.19",
    "bit-docs": "0.0.7",
    "done-serve": "^0.2.0",
    "donejs-cli": "^0.9.4",
    "generator-donejs": "^0.9.0",
    "jshint": "^2.9.1",
    "steal": "^0.16.0",
    "steal-qunit": "^0.1.1",
    "steal-tools": "^1.1.2",
    "testee": "^0.3.0"
  },
  "bit-docs": {
    "dependencies": {
      "bit-docs-glob-finder": "^0.0.5",
      "bit-docs-dev": "^0.0.3",
      "bit-docs-js": "^0.0.3",
      "bit-docs-generate-readme": "^0.0.8"
    },
    "glob": {
      "pattern": "**/*.{js,md}",
      "ignore": "node_modules/**/*"
    },
    "readme": {
      "apis": [
        "can-view-autorender"
      ]
    },
    "parent": "can-view-autorender"
  }
}
